{
  "name": "node-p2pool-scanner",
  "version": "1.0.0",
  "private": true,
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "scripts": {
    "start": "DEBUG='node-p2pool-scanner:* node-p2pool-scanner:*:*' NODE_ENV=development nodemon --exec 'node --use_strict' -w src -w app.js ./app.js",
    "build": "rimraf lib && babel src -d lib --ignore .test.js",
    "devstart": "nodaemon src/ node --prof ./bin/www",
    "test": "eslint --fix src && flow && jest --coverage",
    "port": "flow port",
    "css": "node-sass --recursive --output public/stylesheets/ --source-map true --output-style compressed public/stylesheets/"
  },
  "dependencies": {
    "bluebird": "^3.5.2",
    "debug": "^4.0.1",
    "express": "latest",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-lodash": "^2.4.4",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.44.2",
    "jest": "^19.0.2",
    "lebab": "^2.7.5",
    "licenser": "^0.9.7",
    "node-sass": "^4.9.3",
    "nodemon": "~1.11.0",
    "rimraf": "^2.6.1"
  },
  "description": "Scanner of public P2Pool nodes",
  "main": "bin/www",
  "author": "Maksym Dilai <admin@dproject.org.ua>",
  "license": "GPL-3.0"
}
