{
  "presets": [
    ["env", {
      "targets": {
         "node": "current",
         "browsers": ["last 2 versions"]
        }
    }],
    "flow"
  ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    ["transform-async-to-module-method", {
      "module": "bluebird",
      "method": "coroutine"
    }]
  ]
}
